network:
  version: 2
  renderer: networkd
  ethernets:
    {{ primary_interface }}:
      dhcp4: no
      addresses:
        - {{ target_static_ip }}/{{ target_static_prefix }}
      routes:
        - to: default
          via: {{ target_gateway }}
      nameservers:
        addresses:
{% for server in dns_servers %}
          - {{ server }}
{% endfor %}
